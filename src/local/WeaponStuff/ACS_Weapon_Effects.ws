function ACSGetEquippedSwordPlayEffects()
{
	ACSGetEquippedSword().StopAllEffects();

	if (thePlayer.GetEquippedSign() == ST_Igni)
	{
		ACSGetEquippedSword().PlayEffect('fire_sparks_trail');

		ACSGetEquippedSword().PlayEffect('runeword1_fire_trail');

		ACSGetEquippedSword().PlayEffect('runeword_igni');
	}
	else if (thePlayer.GetEquippedSign() == ST_Axii)
	{
		ACSGetEquippedSword().PlayEffect('ice_sparks_trail');

		ACSGetEquippedSword().PlayEffect('runeword_axii');
	}
	else if (thePlayer.GetEquippedSign() == ST_Aard)
	{
		ACSGetEquippedSword().PlayEffect('aard_power');

		ACSGetEquippedSword().PlayEffect('runeword_aard');
	}
	else if (thePlayer.GetEquippedSign() == ST_Yrden)
	{
		ACSGetEquippedSword().PlayEffect('yrden_power');

		ACSGetEquippedSword().PlayEffect('runeword_yrden');
	}
	else if (thePlayer.GetEquippedSign() == ST_Quen)
	{
		ACSGetEquippedSword().PlayEffect('quen_power');

		ACSGetEquippedSword().PlayEffect('runeword_quen');
	}
}

function igni_sword_summon()
{
	ACSGetEquippedSword().PlayEffect('fire_sparks_trail');

	ACSGetEquippedSword().PlayEffect('runeword1_fire_trail');

	ACSGetEquippedSword().PlayEffect('fast_attack_buff');

	ACSGetEquippedSword().PlayEffect('fast_attack_buff_hit');

	ACSGetEquippedSword().StopEffect('fast_attack_buff');

	ACSGetEquippedSword().StopEffect('fast_attack_buff_hit');

	ACSGetEquippedSword().StopEffect('fire_sparks_trail');

	ACSGetEquippedSword().StopEffect('runeword1_fire_trail');
}

function igni_secondary_sword_summon()
{
	ACSGetEquippedSword().PlayEffect('fire_sparks_trail');

	ACSGetEquippedSword().PlayEffect('runeword1_fire_trail');

	ACSGetEquippedSword().PlayEffect('fast_attack_buff');

	ACSGetEquippedSword().PlayEffect('fast_attack_buff_hit');

	ACSGetEquippedSword().StopEffect('fast_attack_buff');

	ACSGetEquippedSword().StopEffect('fast_attack_buff_hit');

	ACSGetEquippedSword().StopEffect('fire_sparks_trail');

	ACSGetEquippedSword().StopEffect('runeword1_fire_trail');
}

//AXII SWORD EFFECTS//

function axii_sword_stop_effects()
{
	axii_sword_1().StopAllEffects();
	axii_sword_2().StopAllEffects();
	axii_sword_3().StopAllEffects();
	axii_sword_4().StopAllEffects();
	axii_sword_5().StopAllEffects();
}

function axii_sword_trail()
{
	axii_sword_stop_effects();

	axii_sword_1().PlayEffect('heavy_trail_extended_fx');
	axii_sword_2().PlayEffect('heavy_trail_extended_fx');
	axii_sword_3().PlayEffect('heavy_trail_extended_fx');
	axii_sword_4().PlayEffect('heavy_trail_extended_fx');
	axii_sword_5().PlayEffect('heavy_trail_extended_fx');
	
	if (ACS_GetWeaponMode() == 0
	|| ACS_GetWeaponMode() == 1
	|| ACS_GetWeaponMode() == 2)
	{
		if (thePlayer.GetEquippedSign() == ST_Igni)
		{
			axii_sword_1().PlayEffect('fire_sparks_trail');
			axii_sword_2().PlayEffect('fire_sparks_trail');
			axii_sword_3().PlayEffect('fire_sparks_trail');
			axii_sword_4().PlayEffect('fire_sparks_trail');
			axii_sword_5().PlayEffect('fire_sparks_trail');

			axii_sword_1().PlayEffect('runeword1_fire_trail');
			axii_sword_2().PlayEffect('runeword1_fire_trail');
			axii_sword_3().PlayEffect('runeword1_fire_trail');
			axii_sword_4().PlayEffect('runeword1_fire_trail');
			axii_sword_5().PlayEffect('runeword1_fire_trail');

			axii_sword_1().PlayEffect('runeword_igni');
			axii_sword_2().PlayEffect('runeword_igni');
			axii_sword_3().PlayEffect('runeword_igni');
			axii_sword_4().PlayEffect('runeword_igni');
			axii_sword_5().PlayEffect('runeword_igni');
		}
		else if (thePlayer.GetEquippedSign() == ST_Axii)
		{
			axii_sword_1().PlayEffect('ice_sparks_trail');
			axii_sword_2().PlayEffect('ice_sparks_trail');
			axii_sword_3().PlayEffect('ice_sparks_trail');
			axii_sword_4().PlayEffect('ice_sparks_trail');
			axii_sword_5().PlayEffect('ice_sparks_trail');

			axii_sword_1().PlayEffect('runeword_axii');
			axii_sword_2().PlayEffect('runeword_axii');
			axii_sword_3().PlayEffect('runeword_axii');
			axii_sword_4().PlayEffect('runeword_axii');
			axii_sword_5().PlayEffect('runeword_axii');
		}
		else if (thePlayer.GetEquippedSign() == ST_Aard)
		{
			axii_sword_1().PlayEffect('aard_power');
			axii_sword_2().PlayEffect('aard_power');
			axii_sword_3().PlayEffect('aard_power');
			axii_sword_4().PlayEffect('aard_power');
			axii_sword_5().PlayEffect('aard_power');

			axii_sword_1().PlayEffect('runeword_aard');
			axii_sword_2().PlayEffect('runeword_aard');
			axii_sword_3().PlayEffect('runeword_aard');
			axii_sword_4().PlayEffect('runeword_aard');
			axii_sword_5().PlayEffect('runeword_aard');
		}
		else if (thePlayer.GetEquippedSign() == ST_Yrden)
		{
			axii_sword_1().PlayEffect('yrden_power');
			axii_sword_2().PlayEffect('yrden_power');
			axii_sword_3().PlayEffect('yrden_power');
			axii_sword_4().PlayEffect('yrden_power');
			axii_sword_5().PlayEffect('yrden_power');

			axii_sword_1().PlayEffect('runeword_yrden');
			axii_sword_2().PlayEffect('runeword_yrden');
			axii_sword_3().PlayEffect('runeword_yrden');
			axii_sword_4().PlayEffect('runeword_yrden');
			axii_sword_5().PlayEffect('runeword_yrden');
		}
		else if (thePlayer.GetEquippedSign() == ST_Quen)
		{
			axii_sword_1().PlayEffect('quen_power');
			axii_sword_2().PlayEffect('quen_power');
			axii_sword_3().PlayEffect('quen_power');
			axii_sword_4().PlayEffect('quen_power');
			axii_sword_5().PlayEffect('quen_power');

			axii_sword_1().PlayEffect('runeword_quen');
			axii_sword_2().PlayEffect('runeword_quen');
			axii_sword_3().PlayEffect('runeword_quen');
			axii_sword_4().PlayEffect('runeword_quen');
			axii_sword_5().PlayEffect('runeword_quen');
		}
	}
	else if (ACS_GetWeaponMode() == 3)
	{
		ACSGetEquippedSwordPlayEffects();
	}
}

