function yrden_sword_effect_small()
{
	if (thePlayer.IsWeaponHeld('steelsword'))
	{
		if ( ACS_GetWeaponMode() == 0 
		|| ACS_GetWeaponMode() == 1
		|| ACS_GetWeaponMode() == 2 )
		{
			if (ACS_GetArmigerModeWeaponType() == 1
			|| ACS_GetFocusModeWeaponType() == 1
			|| ACS_GetHybridModeWeaponType() == 1)
			{
				yrden_sword_stop_effects();
		
				yrden_sword_1().PlayEffect('absorb_life');

				yrden_sword_1().PlayEffect('summon_shades');

				thePlayer.PlayEffect('heavy_attack_effect_narrow');
				thePlayer.StopEffect('heavy_attack_effect_narrow');
			}
		}
		else if (ACS_GetWeaponMode() == 3)
		{
			if (ACS_GetItem_Caretaker_Shovel())
			{
				ACSGetEquippedSword().StopAllEffects();
					
				ACSGetEquippedSword().StopEffect('summon_shades');
				ACSGetEquippedSword().PlayEffect('summon_shades');

				ACSGetEquippedSword().StopEffect('absorb_life');
				ACSGetEquippedSword().PlayEffect('absorb_life');
				
				thePlayer.PlayEffect('heavy_attack_effect_narrow');
				thePlayer.StopEffect('heavy_attack_effect_narrow');
			}
		}
	}
}

function yrden_sword_effect_big()
{
	if (thePlayer.IsWeaponHeld('steelsword'))
	{
		if ( ACS_GetWeaponMode() == 0 
		|| ACS_GetWeaponMode() == 1
		|| ACS_GetWeaponMode() == 2 )
		{
			if (ACS_GetArmigerModeWeaponType() == 1
			|| ACS_GetFocusModeWeaponType() == 1
			|| ACS_GetHybridModeWeaponType() == 1)
			{
				yrden_sword_stop_effects();
		
				yrden_sword_1().PlayEffect('ground_fx');

				yrden_sword_1().PlayEffect('special_attack_fx');

				yrden_sword_1().PlayEffect('special_trail');

